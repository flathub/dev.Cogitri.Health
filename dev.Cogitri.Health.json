{
    "app-id" : "dev.Cogitri.Health",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "42",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "dev.Cogitri.Health",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--talk-name=org.freedesktop.secrets",
        "--share=network"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "/share/vala",
        "*.la",
        "*.a",
        "/lib/girepository-1.0",
        "/share/gir-1.0",
        "/bin/sassc"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/Health/cargo",
            "RUSTFLAGS" : "-L=/app/lib",
            "RUST_BACKTRACE": "1"
        }
    },
    "modules" : [
        {
            "name" : "Health",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://gitlab.gnome.org/World/Health/-/package_files/63/download",
                    "dest-filename": "Health-0.93.3.tar.xz",
                    "sha256": "0d6286addbf5c9b31917d35f9edda79a10e95c2c903f93676cb648dd3b2b7c2f"
                }
            ]
        }
    ]
}
